[req]
default_bits = 2048
prompt = no
default_md = sha256
req_extensions = req_ext
distinguished_name = dn

[dn]
CN = elasticsearch-master

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = elasticsearch-master
DNS.2 = elasticsearch-master.elk
DNS.3 = elasticsearch-master.elk.svc
DNS.4 = elasticsearch-master.elk.svc.cluster.local
